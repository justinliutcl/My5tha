<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layout_num2"
        >
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:layout_marginTop="@dimen/x10"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="订单信息"
                android:textColor="@color/individualblack"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/x10"
                android:paddingBottom="@dimen/x10"
                android:id="@+id/textView70" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dedede"
                android:id="@+id/textView71" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/main_gry"
                android:id="@+id/orderdetail_layout_goods"
                >

            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/x10"
            android:background="#dedede"
            android:id="@+id/textView72" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:padding="@dimen/x10"
            android:gravity="center_vertical"
            android:id="@+id/goodsorder_layout_coupon"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的优惠卷"
                android:textColor="@color/individualblack"
                android:textSize="@dimen/x12"
                android:id="@+id/textView73" />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="right"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/main_sgry"
                android:textSize="@dimen/x11"
                android:layout_marginRight="@dimen/x15"
                android:id="@+id/goodsorder_coupon" />

            <ImageView
                android:layout_width="@dimen/x8"
                android:layout_height="@dimen/x18"
                android:src="@drawable/icon_arroew_gray3x"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dedede"
            android:id="@+id/textView75" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x10"
            android:background="#fff"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dedede"
                android:id="@+id/textView76" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/y5"
                android:paddingLeft="@dimen/x13"
                android:paddingBottom="@dimen/y5"
                android:paddingRight="@dimen/x12"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="商品金额"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_sgry"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="￥4199.00"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_gry"
                    android:id="@+id/goodsorder_totalmoney" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/y5"
                android:paddingLeft="@dimen/x13"
                android:paddingBottom="@dimen/y5"
                android:paddingRight="@dimen/x12"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="优惠卷"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_sgry"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-￥0.00"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_gry"
                    android:id="@+id/goodsorder_couponmoney" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/y5"
                android:paddingLeft="@dimen/x13"
                android:paddingBottom="@dimen/y5"
                android:paddingRight="@dimen/x12"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="关税"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_sgry"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="￥0.00"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_gry"
                    android:id="@+id/goodsorder_tariffmoney" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/y5"
                android:paddingLeft="@dimen/x13"
                android:paddingBottom="@dimen/y5"
                android:paddingRight="@dimen/x12"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="运费"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_sgry"
                    android:id="@+id/textView57" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="￥0.00"
                    android:textSize="@dimen/x10"
                    android:textColor="@color/main_gry"
                    android:id="@+id/goodsorder_freightmoney" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dedede"
                android:layout_marginTop="@dimen/x5"
                android:id="@+id/textView77" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/x13"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="总计:"
                    android:textSize="@dimen/x11"
                    android:textColor="@color/main_gry"
                    android:id="@+id/textView78" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="￥0.00"
                    android:textColor="@color/main_color"
                    android:textSize="@dimen/x14"
                    android:layout_marginLeft="@dimen/x5"
                    android:id="@+id/goodsorder_summoney" />

                <Button
                    android:layout_width="@dimen/x123"
                    android:layout_height="@dimen/x44"
                    android:text="提交订单"
                    android:textColor="#fff"
                    android:textSize="@dimen/x14"
                    android:background="@drawable/buttonback_nooval"
                    android:id="@+id/goodsorder_commit" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>