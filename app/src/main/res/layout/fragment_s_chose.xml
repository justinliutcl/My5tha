<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#f5f5f5"
    xmlns:wheel="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">
<ListView
    android:id="@+id/frag_chose_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:dividerHeight="1dp"
    android:divider="#fff"
    android:visibility="gone"
    >

</ListView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:id="@+id/myorder_ordernone"
        android:visibility="gone"
        >
        <ImageView
            android:layout_width="@dimen/x65"
            android:layout_height="@dimen/x75"
            android:background="@drawable/order_none"
            android:layout_marginTop="@dimen/x175"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="抱歉，您还没有相关订单"
            android:textSize="@dimen/x13"
            android:textColor="@color/main_sgry"
            android:layout_marginTop="@dimen/x15"
            />
    </LinearLayout>
    <customUI.Loding.ProgressWheel
        android:id="@+id/progress"
        android:layout_width="@dimen/x40"
        android:layout_height="@dimen/x40"
        android:layout_gravity="center"
        android:clickable="false"
        android:layout_centerInParent="true"
        wheel:matProg_barColor="#f32d61"
        wheel:matProg_progressIndeterminate="true" />
</RelativeLayout>